{
  "name": "starterpack",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "subrepo-service-clone": "git subrepo clone git@github.com:Rengod95/ExpoSubRepo-OutboundLib.git src/Service/lib",
    "subrepo-service-push": "git subrepo push src/Service/lib",
    "subrepo-service-pull": "git subrepo pull src/Service/lib",
    "subrepo-component-clone": "git subrepo clone git@github.com:Rengod95/ExpoSubRepo-ComponentLib.git src/app/View/lib",
    "subrepo-component-push": "git subrepo push src/app/View/lib",
    "subrepo-component-pull": "git subrepo pull src/app/View/lib",
    "subrepos-pull": "git subrepo pull src/app/View/lib && git subrepo pull src/Service/lib",
    "subrepos-push": "git subrepo push src/app/View/lib && git subrepo push src/Service/lib"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-firebase/app": "^20.5.0",
    "@react-native-firebase/auth": "^20.5.0",
    "@react-native-firebase/crashlytics": "^20.5.0",
    "@react-native-google-signin/google-signin": "^13.1.0",
    "@react-navigation/native": "^6.0.2",
    "@tamagui/babel-plugin": "^1.111.11",
    "@tamagui/config": "^1.111.11",
    "@tamagui/core": "^1.111.11",
    "@tamagui/lucide-icons": "^1.111.11",
    "axios": "^1.7.7",
    "expo": "~51.0.28",
    "expo-apple-authentication": "~6.4.2",
    "expo-build-properties": "~0.12.5",
    "expo-constants": "~16.0.2",
    "expo-font": "~12.0.10",
    "expo-image-picker": "~15.0.7",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-tracking-transparency": "~4.0.2",
    "expo-web-browser": "~13.0.3",
    "lodash-es": "^4.17.21",
    "mobx": "^6.13.2",
    "mobx-react-lite": "^4.0.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.18.1",
    "react-native-google-mobile-ads": "^14.2.3",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.7.1",
    "react-native-web": "~0.19.10",
    "tamagui": "^1.111.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-unused-imports": "^4.1.3",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.3",
    "prettier": "^3.3.3",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
